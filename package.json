{
  "name": "inversion-tools",
  "version": "0.0.3",
  "description": "This package allows to use IoC with DI by token to class binding",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/*.d.ts",
    "dist/src/*.js",
    "dist/src/*.map"
  ],
  "scripts": {
    "dev": "tsc & node ./dist/src/ioc.js",
    "build": "tsc",
    "test": "jest",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "inversion of control",
    "dependency injection",
    "IoC",
    "DI"
  ],
  "author": "RidiculousCircumstances",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RidiculousCircumstances/inversion-tools.git"
  },
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/uuid": "^8.3.4",
    "jest": "^29.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1"
  }
}
